	<!-- 查询某年某月的登录人数 -->
	<select id="selectByTime" resultType="com.lanou.bean.FactoryRecordBean">
		SELECT TO_CHAR(A.LOGIN_TIME,'YYYY') TIME,COUNT(*) COUNT
FROM LOGIN_LOG_P A  
GROUP BY TO_CHAR(A.LOGIN_TIME,'YYYY')   
ORDER BY TO_CHAR(A.LOGIN_TIME,'YYYY') 

	</select>
	
	<select id="selectByYear" resultType="com.lanou.bean.FactoryRecordBean" parameterType="java.lang.String">
	
	select to_char(t.login_time,'mm') TIME,count(t.login_log_id) COUNT
		from LOGIN_LOG_P t
		where to_char(t.login_time,'YYYY') =#{year}
		group by to_char(t.login_time,'mm')
		order by to_char(t.login_time,'mm')
	</select>



